<?xml version="1.0" ?>
<sdf version="1.5">
  <world name="default">

    <include>
      <uri>model://sun</uri>
    </include>

    <include>
      <uri>model://ground_plane</uri>
    </include>


    <include>
      <uri>model://motor</uri>
      <pose>1.0 -0.2 0.92 0 0 0</pose>
    </include>


    <include>
      <uri>model://car</uri>
      <pose>0 0 0.55 0 0 0</pose>
    </include>

    <model name="gearbox">
      <pose>1.0 0.2 0.92 0 0 0</pose>
      <link name="gearbox_link">
        <collision name="gearbox_collision">
          <geometry>
            <box>
              <size>0.15 0.15 0.15</size>
            </box>
          </geometry>
        </collision>
        <visual name="gearbox_visual">
          <geometry>
            <box>
              <size>0.15 0.15 0.15</size>
            </box>
          </geometry>
          <material>
            <script>
              <name>Gazebo/Red</name>
            </script>
          </material>
        </visual>
      </link>

      <plugin filename="libGearboxPlugin.so" name="gearbox">
        <properties>
          <parent>motor::motor_shaft_link</parent>
          <child>car::axle</child>
          <gear_ratio>1.0</gear_ratio>
        </properties>
      </plugin>

    </model>


