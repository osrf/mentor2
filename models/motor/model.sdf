<?xml version="1.0" ?>
<sdf version="1.5">
  <model name="motor">
    <pose>0 0 0.1 0 0 0</pose>
    <link name="link">
      <pose>0.0 0 0.0 0 0 1.57079632679</pose>
      <self_collide>false</self_collide>
      <inertial>
        <mass>3</mass>
        <inertia>
          <ixx>0.01</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.01</iyy>
          <iyz>0</iyz>
          <izz>0.01</izz>
        </inertia>
      </inertial>

      <collision name="collision">
        <pose>0 0 0.0 0 0 0</pose>
        <geometry>
          <mesh>
            <uri>model://motor/meshes/motor.dae</uri>
            <scale>0.01 0.01 0.01</scale>
          </mesh>
        </geometry>
      </collision>
      <visual name="visual">
        <pose>0 0 0.0 0 0 0</pose>
        <geometry>
          <mesh>
            <uri>model://motor/meshes/motor.dae</uri>
            <scale>0.01 0.01 0.01</scale>
          </mesh>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/Grey</name>
          </script>
        </material>
      </visual>
    </link>
<!--
    <joint name="world_joint" type="revolute">
      <parent>world</parent>
      <child>link</child>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>0</lower>
          <upper>0</upper>
        </limit>
      </axis>
    </joint>
-->
    <!--
    <joint name="wheel_joint" type="revolute">
      <parent>link</parent>
      <child>bucket_wheel::link</child>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>-1000000000</lower>
          <upper>1000000000</upper>
        </limit>
      </axis>
    </joint>
-->

    <link name="motor_core_link">
      <pose>0.0 0 0.0 0 0 0</pose>
      <self_collide>false</self_collide>
      <gravity>false</gravity>
      <inertial>
        <mass>0.1</mass>
        <inertia>
          <ixx>0.001</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.001</iyy>
          <iyz>0</iyz>
          <izz>0.001</izz>
        </inertia>
      </inertial>
      <collision name="collision">
        <pose>0.0 0 0.0 1.57079632679 0 0</pose>
        <geometry>
          <cylinder>
            <radius>0.05</radius>
            <length>0.05</length>
          </cylinder>
        </geometry>
      </collision>
      <visual name="visual">
        <pose>0.0 0 0.0 1.57079632679 0 0</pose>
        <geometry>
          <cylinder>
            <radius>0.05</radius>
            <length>0.05</length>
          </cylinder>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/Grey</name>
          </script>
        </material>
      </visual>
    </link>

    <link name="motor_shaft_link">
      <pose>0.0 0 0.0 0 0 0</pose>
      <self_collide>false</self_collide>
      <gravity>false</gravity>
      <inertial>
        <mass>0.05</mass>
        <inertia>
          <ixx>0.0001</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.0001</iyy>
          <iyz>0</iyz>
          <izz>0.0003</izz>
        </inertia>
      </inertial>
      <visual name='debug_vis'>
        <pose>0.0 0.12 0.0 0 0.0 0.0</pose>
        <geometry>
          <box>
            <size>.02 .02 .02</size>
          </box>
        </geometry>
      </visual>
      <collision name="collision">
        <pose>0.0 0 0.0 1.57079632679 0 0</pose>
        <geometry>
          <cylinder>
            <radius>0.005</radius>
            <length>0.20</length>
          </cylinder>
        </geometry>
      </collision>
      <visual name="visual">
        <pose>0.0 0 0.0 1.57079632679 0 0</pose>
        <geometry>
          <cylinder>
            <radius>0.005</radius>
            <length>0.20</length>
          </cylinder>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/Wood</name>
          </script>
        </material>
      </visual>
      <velocity_decay>
        <angular>0.0001</angular>
      </velocity_decay>
    </link>

    <joint name="motor_shaft_joint" type="revolute">
      <parent>motor::motor_core_link</parent>
      <child>motor::motor_shaft_link</child>
      <axis>
        <xyz>0 1 0</xyz>
        <limit>
          <lower>-1000000</lower>
          <upper>1000000</upper>
        </limit>
        <dynamics>
          <friction>0.000001</friction>
          <damping>0.0001</damping>
        </dynamics>
      </axis>
    </joint>

    <joint name="motor_joint" type="revolute">
      <parent>motor::motor_core_link</parent>
      <child>motor::link</child>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>0</lower>
          <upper>0</upper>
        </limit>
      </axis>
    </joint>

    <plugin filename="libMotorPlugin.so" name="motor">
      <port>
        <name>shaft</name>
        <type>mechanical</type>
      </port>
      <port>
        <name>connector0</name>
        <type>electrical</type>
      </port>
      <port>
        <name>connector1</name>
        <type>electrical</type>
      </port>

      <properties>
        <resistance>5</resistance>
        <back_emf>0.0064</back_emf>
        <torque_constant>0.0064</torque_constant>
      </properties>

      <shaft_joint>motor_shaft_joint</shaft_joint>
    </plugin>
  </model>
</sdf>
