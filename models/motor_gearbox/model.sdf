<?xml version="1.0" ?>
<sdf version="1.5">

 <model name="motor_gearbox">

    <model name="motor">
<!--      <pose>0 0.013 0.0285 0 0 0</pose>-->
      <pose>-0.38 0 0.1 0 0 0</pose>
      <link name="link">
        <pose>0.0 0 0.0 0 0 0</pose>
        <inertial>
          <mass>3</mass>
          <inertia>
            <ixx>0.01</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.01</iyy>
            <iyz>0</iyz>
            <izz>0.01</izz>
          </inertia>
        </inertial>
        <self_collide>false</self_collide>
        <collision name="collision">
          <pose>0.0 0 0.0 0 0 0</pose>
          <geometry>
            <box>
              <size>0.258 0.155 0.198</size>
            </box>
          </geometry>
        </collision>
        <visual name="visual">
          <pose>0 0 0.0 0 0 0</pose>
          <geometry>
            <mesh>
              <uri>model://motor/meshes/motor.dae</uri>
              <scale>0.01 0.01 0.01</scale>
            </mesh>
          </geometry>
          <material>
            <script>
              <uri>file://media/materials/scripts/gazebo.material</uri>
              <name>Gazebo/Grey</name>
            </script>
          </material>
        </visual>
      </link>

      <link name="motor_core_link">
        <pose>0.002 0 0.0 0 0 0</pose>
        <self_collide>false</self_collide>
        <gravity>false</gravity>
        <inertial>
          <mass>0.1</mass>
          <inertia>
            <ixx>0.0001</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.0001</iyy>
            <iyz>0</iyz>
            <izz>0.0001</izz>
          </inertia>
        </inertial>
        <collision name="collision">
          <pose>0.0 0 0.0 0 1.57079632679 0</pose>
          <geometry>
            <cylinder>
              <radius>0.05</radius>
              <length>0.05</length>
            </cylinder>
          </geometry>
        </collision>
        <visual name="visual">
          <pose>0.0 0 0.0 0 1.57079632679 0</pose>
          <geometry>
            <cylinder>
              <radius>0.05</radius>
              <length>0.05</length>
            </cylinder>
          </geometry>
          <material>
            <script>
              <uri>file://media/materials/scripts/gazebo.material</uri>
              <name>Gazebo/Grey</name>
            </script>
          </material>
        </visual>
      </link>

      <link name="motor_shaft_link">
        <pose>0.002 0 0.0 0 0 0</pose>
        <self_collide>false</self_collide>
        <gravity>false</gravity>
          <inertial>
            <mass>0.05</mass>
            <inertia>
              <ixx>0.0001</ixx>
              <ixy>0</ixy>
              <ixz>0</ixz>
              <iyy>0.0001</iyy>
              <iyz>0</iyz>
              <izz>0.0003</izz>
            </inertia>
          </inertial>
        <collision name="collision">
          <pose>0.0 0 0.0 0 1.57079632679 0</pose>
          <geometry>
            <cylinder>
              <radius>0.005</radius>
              <length>0.20</length>
            </cylinder>
          </geometry>
        </collision>
        <visual name='debug_vis'>
          <pose>0.15 0.0 0.0 0 0.0 0.0</pose>
          <geometry>
            <box>
              <size>.02 .02 .02</size>
            </box>
          </geometry>
        </visual>

        <visual name="visual">
          <pose>0.0 0 0.0 0 1.57079632679 0</pose>
          <geometry>
            <cylinder>
              <radius>0.005</radius>
              <length>0.20</length>
            </cylinder>
          </geometry>
          <material>
            <script>
              <uri>file://media/materials/scripts/gazebo.material</uri>
              <name>Gazebo/Wood</name>
            </script>
          </material>
        </visual>
        <velocity_decay>
          <angular>0.0001</angular>
        </velocity_decay>
      </link>

      <joint name="motor_shaft_joint" type="revolute">
        <parent>motor_core_link</parent>
        <child>motor_shaft_link</child>
        <axis>
          <xyz>1 0 0</xyz>
          <limit>
            <lower>-1000000</lower>
            <upper>1000000</upper>
          </limit>
          <dynamics>
            <friction>0.000001</friction>
            <damping>0.0001</damping>
          </dynamics>
        </axis>
      </joint>

      <joint name="motor_joint" type="revolute">
        <parent>motor_core_link</parent>
        <child>link</child>
        <axis>
          <xyz>0 0 1</xyz>
          <limit>
            <lower>0</lower>
            <upper>0</upper>
          </limit>
        </axis>
      </joint>

      <plugin filename="libMotorPlugin.so" name="motor">
        <port>
          <name>shaft</name>
          <type>mechanical</type>
        </port>
        <port>
          <name>connector0</name>
          <type>electrical</type>
        </port>
        <port>
          <name>connector1</name>
          <type>electrical</type>
        </port>

        <properties>
          <resistance>5</resistance>
          <back_emf>0.0064</back_emf>
          <torque_constant>0.0064</torque_constant>
        </properties>

        <shaft_joint>motor_shaft_joint</shaft_joint>
      </plugin>
    </model>


    <model name="axle_wheels">
      <pose>0.24 0.28 0.15 0 0 0</pose>
      <link name="link">
        <inertial>
          <mass>0.1</mass>
          <inertia>
            <ixx>0.0001</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.0001</iyy>
            <iyz>0</iyz>
            <izz>0.0003</izz>
          </inertia>
        </inertial>
        <collision name="collision">
          <pose>0 0 0.0 0 1.57079632679 0</pose>
          <geometry>
            <cylinder>
              <radius>0.10</radius>
              <length>0.10</length>
            </cylinder>
          </geometry>
        </collision>
        <visual name="visual">
          <pose>0 0.0 0.00 0  1.57079632679 0</pose>
          <geometry>
            <cylinder>
              <radius>0.1</radius>
              <length>0.1</length>
            </cylinder>
          </geometry>
          <material>
            <script>
              <uri>file://media/materials/scripts/gazebo.material</uri>
              <name>Gazebo/Grey</name>
            </script>
          </material>
        </visual>
        <visual name='debug_vis'>
          <pose>0.06 0.0 0.0 0 0.0 0.0</pose>
          <geometry>
            <box>
              <size>.02 .02 .02</size>
            </box>
          </geometry>
        </visual>
      </link>
    </model>


    <model name="gearbox">
      <pose>0 0.0 0.15 0 0 0</pose>
      <link name="link">
        <inertial>
          <mass>5</mass>
          <inertia>
            <ixx>0.1</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.1</iyy>
            <iyz>0</iyz>
            <izz>0.1</izz>
          </inertia>
        </inertial>
        <collision name="collision">
<!--          <pose>0 0.0 0.12 1.57079632679 0 1.57079632679</pose>-->
          <pose>0 0.09 0.0 0 0 0</pose>
          <geometry>
            <box>
<!--              <size>0.5 0.01 0.4</size>-->
              <size>0.365 0.70 0.270</size>
            </box>
          </geometry>
        </collision>
        <visual name="visual">
          <pose>0 0 0.0 1.57079632679 0 1.57079632679</pose>
          <geometry>
            <mesh>
              <uri>model://gearbox/meshes/gearbox.dae</uri>
              <scale>0.01 0.01 0.01</scale>
            </mesh>
          </geometry>
          <material>
            <script>
              <uri>file://media/materials/scripts/gazebo.material</uri>
              <name>Gazebo/Grey</name>
            </script>
          </material>
        </visual>
      </link>
      <plugin filename="libGearboxPlugin.so" name="gearbox">
        <properties>
          <parent>axle_wheels::link</parent>
          <child>motor::motor_shaft_link</child>
          <gear_ratio>1.0</gear_ratio>
          <efficiency>0.95</efficiency>
        </properties>
      </plugin>
    </model>

    <joint name="gearbox_motor_joint" type="revolute">
      <parent>gearbox::link</parent>
      <child>motor::link</child>
      <axis>
        <xyz>1 0 0</xyz>
        <limit>
          <lower>0</lower>
          <upper>0</upper>
        </limit>
      </axis>
    </joint>

    <joint name="gearbox_axle_joint" type="revolute">
      <parent>gearbox::link</parent>
      <child>axle_wheels::link</child>
      <axis>
        <xyz>1 0 0</xyz>
        <limit>
          <lower>-1000000000000000</lower>
          <upper>1000000000000000</upper>
        </limit>
      </axis>
    </joint>
  </model>

</sdf>
